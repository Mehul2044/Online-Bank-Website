<%- include("partials/header") %>
<title><%= projectName %></title>
<link rel="stylesheet" href="/css/registration.css">
<script src="/js/registration_validate.js" defer></script>
</head>

<body>

<%-include("partials/nav_before_login") %>

<form action="/registration" method="post" enctype="multipart/form-data">
    <label for="fName">First Name:</label>
    <input type="text" id="fName" name="firstName" required placeholder="First Name">
    <label for="lName">Last Name:</label>
    <input type="text" id="lName" name="lastName" required placeholder="Last Name">
    <label for="eMail">Email:</label>
    <input type="email" id="eMail" name="eMail" required placeholder="abc123@sample.com">
    <label for="file">Upload the Account Opening Form in a PDF File:</label>
    <input type="file" id="file" name="file" style="color: white;" required accept="application/pdf">
    <input type="submit" value="Register">
    <a href="ac-opening-individual.pdf" target="_blank">Download Account Opening Form</a>
</form>

<%- include("partials/footer") %>

</body>
</html>